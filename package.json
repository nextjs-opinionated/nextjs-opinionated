    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "classnames": "^2.3.1",
    "daisyui": "^1.16.0",
    "dayjs": "^1.10.7",
    "fs-extra": "^10.0.0",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "next": "12.0.1",
    "next-themes": "^0.0.15",
    "pino": "^7.0.5",
    "pino-pretty": "^7.1.0",
    "query-string": "^7.0.1",
    "react": "17.0.2",
    "react-colorful": "^5.5.0",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.18.0",
    "react-icons": "^4.3.1",
    "react-query": "^3.29.0",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "remark-prism": "^1.3.6",
    "sweetalert2": "^11.1.9",
    "sweetalert2-react-content": "^4.2.0",
    "uuid": "^8.3.2",
    "validator": "^13.6.0",
    "vfile": "^5.1.1",
    "zod": "^3.11.6"
  },
