{
  "name": "nextjs-opinionated",
  "version": "0.3.0",
  "private": false,
  "author": {
    "email": "saitodisse@gmail.com",
    "name": "julio saito",
    "url": "https://nextjs-opinionated.vercel.app/"
  },
  "keywords": [
    "boilerplate",
    "daisyui",
    "jest",
    "next.js",
    "react",
    "react-hook-form",
    "storybook",
    "tailwindcss",
    "typescript",
    "validator",
    "zod"
  ],
  "scripts": {
    "dev": "next dev | pino-pretty",
    "build": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "export": "next export",
    "test": "jest --env=jsdom --modulePathIgnorePatterns='\\.integration\\.'",
    "test:integration": "jest --env=node",
    "test:ci": "yarn test --runInBand --no-cache",
    "storybook": "start-storybook --ci -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "chromatic": "chromatic --exit-zero-on-changes",
    "lint": "next lint --fix -c ./.eslintrc.js src",
    "checkAll": "yarn lint && yarn test:ci && yarn build && yarn storybook --smoke-test",
    "push": "yarn checkAll && git push"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.6.1",
    "@headlessui/react": "^1.4.1",
    "@hookform/resolvers": "^2.8.2",
