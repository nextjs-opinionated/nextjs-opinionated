    "typescript": "4.4.4",
    "zod": "^3.11.6"
  },
  "resolutions": {
    "react": "17.0.2"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/",
      "./.storybook/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgrMock.js"
    }
  }
}
